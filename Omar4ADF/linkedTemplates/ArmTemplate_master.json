{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Omar4ADF"
		},
		"Bloblink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Bloblink'"
		},
		"SQLlink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLlink'"
		},
		"Customercsv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Customercsv'"
		},
		"csv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'csv'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Omar4ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Bloblink_connectionString": {
						"value": "[parameters('Bloblink_connectionString')]"
					},
					"SQLlink_connectionString": {
						"value": "[parameters('SQLlink_connectionString')]"
					},
					"Customercsv_connectionString": {
						"value": "[parameters('Customercsv_connectionString')]"
					},
					"csv_connectionString": {
						"value": "[parameters('csv_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}